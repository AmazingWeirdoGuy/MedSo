{
  "routes": [
    {
      "src": "/admin/config.yml",
      "dest": "/admin/config.yml"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/content/(.*)",
      "dest": "/content/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
